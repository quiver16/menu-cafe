{
    "compilerOptions": {
        "outDir": "dist",
        "rootDir": "./src",
        "lib": [
            "ESNext"
        ],
        "target": "ES2022",
        "moduleResolution": "bundler",
        "module": "esnext",
        "strict": false,
        "sourceMap": true,
        "esModuleInterop": true,
        "declaration": true,
    },
    "include": [
        "src/**/*.ts"
    ]
}